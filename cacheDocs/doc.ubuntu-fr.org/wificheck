&nbsp;
<span class="co0"># lspci -k -nn | grep -A 3 -i net</span>
<span class="kw3">echo</span> <span class="re5">-e</span> <span class="st0">&quot;<span class="es1">\n</span>&gt;&gt;    lspci -k -nn | grep -A 3 -i net <span class="es1">\n</span>&quot;</span>
<span class="kw2">lspci</span> <span class="re5">-k</span> <span class="re5">-nn</span> <span class="sy0">|</span> <span class="kw2">grep</span> <span class="re5">-A</span> <span class="nu0">3</span> <span class="re5">-i</span> net
&nbsp;
<span class="co0"># sudo lshw -C network</span>
<span class="kw3">echo</span> <span class="re5">-e</span> <span class="st0">&quot;<span class="es1">\n</span>&gt;&gt;    sudo lshw -C network <span class="es1">\n</span>&quot;</span>
<span class="kw2">sudo</span> lshw <span class="re5">-C</span> network
&nbsp;
<span class="co0"># lsmod</span>
<span class="kw3">echo</span> <span class="re5">-e</span> <span class="st0">&quot;<span class="es1">\n</span>&gt;&gt;    lsmod <span class="es1">\n</span>&quot;</span>
<span class="kw2">lsmod</span>
&nbsp;
<span class="kw3">echo</span> <span class="br0">&#91;</span><span class="sy0">/</span>code<span class="br0">&#93;</span>
&nbsp;
<span class="kw3">echo</span> <span class="br0">&#91;</span>code<span class="br0">&#93;</span>
&nbsp;
<span class="co0"># iwconfig</span>
<span class="kw3">echo</span> <span class="re5">-e</span> <span class="st0">&quot;<span class="es1">\n</span>&gt;&gt;    iwconfig <span class="es1">\n</span>&quot;</span>
iwconfig
&nbsp;
<span class="co0"># ifconfig -a (merci à toutafai)</span>
