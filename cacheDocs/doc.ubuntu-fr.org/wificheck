&nbsp;
<span class="kw3">exec</span> <span class="nu0">3</span><span class="sy0">&gt;&amp;</span><span class="nu0">1</span>
<span class="kw3">exec</span> <span class="nu0">1</span><span class="sy0">&gt;</span>~<span class="sy0">/</span>wificheck.log
&nbsp;
<span class="kw3">echo</span> <span class="br0">&#91;</span>code<span class="br0">&#93;</span>
&nbsp;
<span class="co0"># cat /etc/lsb-release</span>
<span class="kw3">echo</span> <span class="re5">-e</span> <span class="st0">&quot;<span class="es1">\n</span>&gt;&gt;    cat /etc/lsb-release <span class="es1">\n</span>&quot;</span>
