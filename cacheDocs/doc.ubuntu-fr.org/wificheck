<span class="co0"># uname -r -m</span>
<span class="kw3">echo</span> <span class="re5">-e</span> <span class="st0">&quot;<span class="es1">\n</span>&gt;&gt;    uname -r -m <span class="es1">\n</span>&quot;</span>
